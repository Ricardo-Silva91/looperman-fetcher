{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "import/extensions": "off",
        "no-console": "off",
        "no-await-in-loop": "off"
    },
    "ignorePatterns": ["**/playwright-helpers/*"]
}
